<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Custom Settings for the Installed Products Page in Apttus CPQ</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AmendChangeFields__c</fullName>
        <deprecated>false</deprecated>
        <description>Comma separated line item field names whose values can turn an existing asset line into amended asset line.</description>
        <externalId>false</externalId>
        <inlineHelpText>Comma separated line item field names whose values can turn an existing asset line into amended asset line.</inlineHelpText>
        <label>Amend Change Fields</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>DefaultRenewalTerm__c</fullName>
        <defaultValue>0</defaultValue>
        <deprecated>false</deprecated>
        <description>When the value is provided use this value as the renewal term for Renewing products.  Number signifies months to renew as the end date.</description>
        <externalId>false</externalId>
        <inlineHelpText>When the value is provided use this value as the renewal term for Renewing products.  Number signifies months to renew as the end date.</inlineHelpText>
        <label>Default Renewal Term</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>FilterFields__c</fullName>
        <deprecated>false</deprecated>
        <description>Asset Line Fields to be displayed on filter.   Works with text fields and End Date.</description>
        <externalId>false</externalId>
        <inlineHelpText>Asset Line Fields to be displayed on filter.  Works with text fields and End Date.</inlineHelpText>
        <label>Filter Fields</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>HideCoTerm__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check to hide co-term end date on renew</description>
        <externalId>false</externalId>
        <inlineHelpText>Check to hide co-term end date on renew</inlineHelpText>
        <label>Hide Co-Term</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>MaxRenewsPerTrip__c</fullName>
        <defaultValue>20</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates the number of renewals that are processed before a server roundtrip is required.</description>
        <externalId>false</externalId>
        <inlineHelpText>To prevent CPU timeout use a smaller number. Default is 20</inlineHelpText>
        <label>Max Renews Per Trip</label>
        <precision>5</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ShowAccountsFilter__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Display accounts filter in the installed products page</description>
        <externalId>false</externalId>
        <inlineHelpText>Display accounts filter in the installed products page</inlineHelpText>
        <label>Show Accounts Filter</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Installed Products Settings</label>
</CustomObject>

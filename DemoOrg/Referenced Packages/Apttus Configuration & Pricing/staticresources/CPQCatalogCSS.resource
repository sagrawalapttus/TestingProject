/*HOVER CSS MENU*/



#nav, #nav li { 
	margin:0px; 
	padding:0px; 
}
#nav li {  
	cursor:pointer; 
	list-style:none; 
	padding:0px 10px 0px 10px; 
	border:1px #000 solid; 
	position:relative;
}
#nav li ul.first {
	left:-1px; 
	top:100%;
}

/*li, li a {
	color:#000; 
	text-decoration:none;
}*/
#nav li {
	text-indent:10px;  
	/*margin-right:10px;*/ 
	border-top:1px #000 solid; 
	border-bottom:1px #000 solid; 
	border-left:none; 
	border-right:none; 
	background:#fff;
}
#nav li a {
	display:block; 
	width:auto; 
	height:inherit;
	font-weight: normal;
}

.firstnav li:hover > a, .firstnav li:hover { 
	color:#fff; 
	background:#1b9bff !important;  
}

/*#stuff li {list-style:none;}*/

#nav, .firstnav, #nav .firstnav li { 
	margin:0px; 
	padding:0px; 
}
#nav li {  
	cursor:pointer; 
	list-style:none; 
	padding:0px 10px 0px 10px; 
	border:1px #000 solid; 
	position:relative;
}

#nav li ul.first {
	left:-1px; 
	top:100%;
}

/*li, li a {
	color:#000; 
	text-decoration:none;
}*/

#nav .firstnav li { 
	width:100%; 
	text-indent:10px;  
	/*margin-right:10px;*/ 
	border-top:1px #000 solid; 
	border-bottom:1px #000 solid; 
	border-left:none; border-right:none; 
	background:#333;
}

#nav li a {
	display:block;
	/*width:inherit;*/ 
	height:inherit;
	color: #fff;
}

ul.firstnav { 
	display:none; 
}

li:hover > .firstnav { 
	display:block; 
	position:absolute; 
	width:200px; 
	top:30px; 
	left:0; 
	z-index:1000; 
	background:#1b9bff !important; 
	border:1px #000 solid; 
} 
nav.li:hover { 
	position:relative;
	
	z-index:2000; 
}


/***************************
*  CAROUSSEL 
**************************/
.image_carousel {
	padding: 0 25px 0 25px;
	position: relative;
}

.image_carousel div.aptCarrouselItem  { 
	background-color: white;
	padding: 5px;
	margin: 5px;
	display: block;
	float: left;
	box-shadow: rgb(204, 204, 204) 2px 3px 5px;
}
.image_carousel div.aptCarrouselItem:hover {
	
	box-shadow: 0px 1px 5px #ccc;
	transition: all 0.25s ease-in-out;
	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: all 0.25s ease-in-out;
	border-radius: 3px;
	border: 1px solid rgb(30, 148, 226);
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	box-shadow: 0px 0px 15px rgba(67, 99, 139, 1) !important;
}

.image_carousel div.aptCarrouselItemLabel  { 
	background-color: white;
	display:block;
	position: relative;
	bottom: 0;
	margin-bottom: 0;
	padding-bottom: 0;
}


/* .aptCarrouselItem:hover {
	
	background-color: gray;
	/* border: 2px solid gray; 
	cursor: pointer;
	box-shadow: none;
} */

.aptCarrouselItemActive{

	box-shadow: 0px 1px 2px #ccc !important;
	transition: all 0.25s ease-in-out;
	-webkit-transition: all 0.25s ease-in-out;
	-moz-transition: all 0.25s ease-in-out;
	border-radius: 3px;
	border: 1px solid rgb(30, 148, 226);
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	box-shadow: 0px 0px 15px rgba(67, 99, 139, 1) !important;
}
		
a.prev, a.next {
	background: url('{!$Resource.CPQCarouselArrowsSprite}') no-repeat transparent;
	width: 40px;
	height: 100px;
	display: block;
	position: absolute;
	top: 60px;
}

a.prev {			
	left: 0px;
	background-position: -60px -83px; 
}

a.prev:hover {		
	background-position: 0px -83px; 
}

a.prev.disabled {	
	background-position: -100px -100px !important;  
}

a.next {			
	right: 0px;
	background-position: -60px -183px; 
}

a.next:hover {	
	background-position: 0px -183px; 	
	
}

a.next.disabled {	
	background-position: -100px -100px !important;  
}

a.prev.disabled, a.next.disabled {
	cursor: default;
}

a.prev span, a.next span {
	display: none;
}

.pagination {
	text-align: center;
}

.pagination a {
	background: url('{!$Resource.Caroussel_Sprite}') 0 -100px no-repeat transparent;
	width: 15px;
	height: 15px;
	margin: 0 5px 0 0;
	display: inline-block;
}
 
.pagination a.selected {
	background-position: -25px -100px;
	cursor: default;
}

.pagination a span {
	display: none;
}

.clearfix {
	float: none;
	clear: both;
}

